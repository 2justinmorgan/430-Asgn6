function value = numopV(op, interpedArgs)
if nargin == 2
    left = interpedArgs(1);
    right = interpedArgs(2);
    switch class(left)
        case 'numV'
            switch class(right)
                case 'numV'
                    switch op
                        case "+"
                            value = numV(left.number + right.number);
                        case "-"
                            value = numV(left.number - right.number);
                        case "*"
                            value = numV(left.number * right.number);
                        case "/"
                            value = numV(left.number / right.number);
                        case "<="
                            value = boolV(left.number <= right.number);
                        otherwise
                            disp(op)
                            error("invalid op");
                    end
                otherwise
                    disp(right);
                    error("right val must be numV for numeric ops");
            end
        otherwise
            disp(left);
            error("left val must be numV for numeric ops");
    end
end