classdef ifC
    properties
       testVal {numC}
       thenVal {numC}
       elseVal {numC}
    end
    methods
        function obj = ifC(testv, thenv, elsev)
            if nargin == 3
                obj.testVal = testv;
                obj.thenVal = thenv;
                obj.elseVal = elsev;              
            end
        end
    end
end